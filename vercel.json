{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "dist/**",        "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)",      "dest": "/api/$1.js" },
    { "src": "/(.*)",          "dest": "/dist/$1" },
    { "src": "/",              "dest": "/dist/index.html" }
  ]
}
git add dist/ vercel.json
git commit -m "chore: commit prebuilt dist and serve static on Vercel"
git push
